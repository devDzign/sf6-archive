{% extends 'base.html.twig' %}

{% block title %}Address{% endblock %}

{% block body %}
    <h1>Address</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ address.id }}</td>
            </tr>
            <tr>
                <th>StreetNumber</th>
                <td>{{ address.streetNumber }}</td>
            </tr>
            <tr>
                <th>StreetType</th>
                <td>{{ address.streetType }}</td>
            </tr>
            <tr>
                <th>StreetName</th>
                <td>{{ address.streetName }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ address.city }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ address.zipCode }}</td>
            </tr>
        </tbody>
    </table>



    {{ include('address/_delete_form.html.twig') }}
{% endblock %}
